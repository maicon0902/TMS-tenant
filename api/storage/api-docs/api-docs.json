    "paths": {
        "/api/customers/{customerId}/contacts": {
            "get": {
                "tags": [
                    "Contacts"
                ],
                "summary": "Get contacts for a customer",
                "operationId": "a3402bade8ef225bee56fb80ff6e44ee",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Customer ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of contacts"
                    },
                    "404": {
                        "description": "Customer not found"
                    }
                }
            },
            "post": {
                "tags": [
                    "Contacts"
                ],
                "summary": "Create a new contact for a customer",
                "operationId": "d88f0b638f6419587cfb1ced81e97032",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Customer ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "first_name"
                                ],
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "example": "John"
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "example": "Doe",
                                        "nullable": true
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Contact created successfully"
                    },
                    "404": {
                        "description": "Customer not found"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                }
            }
        },
